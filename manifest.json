{
    "manifest_version": 3,
    "name": "Pinterest Scanner",
    "version": "1.0",
    "description": "Scan Pinterest Pins for info regarding it.",
    "permissions": ["storage"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon.png",
            "48": "icon.png",
            "128": "icon.png"
        }
    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    }
}
